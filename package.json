{
  "name": "@financial-times/n-es-tools",
  "version": "0.0.0",
  "dependencies": {
    "@elastic/elasticsearch": "~7.9.0",
    "async-sema": "^3.0.0",
    "aws-elasticsearch-connector": "^9.0.3",
    "aws-sdk": "^2.124.0",
    "chalk": "^2.3.0",
    "commander": "^3.0.0",
    "http-aws-es": "^4.0.0",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.6.6",
    "progress": "^2.0.0",
    "s3-client": "^4.4.2"
  },
  "scripts": {
    "postinstall": "node bin/tools install",
    "prepare": "npx snyk protect || npx snyk protect -d || true",
    "preinstall": "[ \"$INIT_CWD\" != \"$PWD\" ] || npm_config_yes=true npx check-engine",
    "precommit": "secret-squirrel",
    "commitmsg": "secret-squirrel-commitmsg",
    "prepush": "make verify -j3"
  },
  "bin": {
    "n-es-tools": "bin/tools"
  },
  "devDependencies": {
    "@financial-times/n-gage": "^8.3.2",
    "check-engines": "^1.5.0",
    "npm-prepublish": "^1.2.2",
    "snyk": "^1.167.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": "12.x",
    "npm": "7.x || 8.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node_modules/.bin/secret-squirrel-commitmsg",
      "pre-commit": "node_modules/.bin/secret-squirrel",
      "pre-push": "make verify -j3"
    }
  },
  "volta": {
    "node": "12.22.5",
    "npm": "7.20.2"
  }
}
