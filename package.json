{
  "name": "@financial-times/n-es-tools",
  "version": "0.0.0",
  "dependencies": {
    "@elastic/elasticsearch": "~7.9.0",
    "async-sema": "^3.0.0",
    "aws-elasticsearch-connector": "^9.0.3",
    "aws-sdk": "^2.124.0",
    "chalk": "^2.3.0",
    "commander": "^3.0.0",
    "http-aws-es": "^4.0.0",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "progress": "^2.0.0",
    "s3-client": "^4.4.2"
  },
  "scripts": {
    "postinstall": "node bin/tools install",
    "prepare": "npx snyk protect || npx snyk protect -d || true",
    "preinstall": "[ \"$INIT_CWD\" != \"$PWD\" ] || npm_config_yes=true npx check-engine",
    "build": "dotcom-tool-kit build:local",
    "test": "dotcom-tool-kit test:local",
    "start": "dotcom-tool-kit run:local"
  },
  "bin": {
    "n-es-tools": "bin/tools"
  },
  "devDependencies": {
    "@dotcom-tool-kit/circleci-npm": "^2.0.9",
    "@dotcom-tool-kit/eslint": "^2.2.1",
    "@dotcom-tool-kit/husky-npm": "^2.2.0",
    "@dotcom-tool-kit/secret-squirrel": "^1.0.7",
    "@financial-times/eslint-config-next": "^7.0.0",
    "check-engine": "^1.10.1",
    "dotcom-tool-kit": "^2.3.5",
    "npm-prepublish": "^1.2.2",
    "snyk": "^1.167.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x || 8.x"
  },
  "volta": {
    "node": "16.14.1",
    "npm": "7.20.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "dotcom-tool-kit git:commitmsg",
      "pre-commit": "dotcom-tool-kit git:precommit"
    }
  }
}
